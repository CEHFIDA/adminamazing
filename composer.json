{
    "name": "selfreliance/adminamazing",
    "type": "library",
    "version": "1.0.2",
    "description": "Admin for service laravel",
    "keywords": ["admin", "laravel"],
    "homepage": "https://github.com/selfrelianceme/adminamazing",
    "license": "MIT",
    "authors": [
        {
            "name": "Vlad Kit",
            "email": "vladwebkit@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {"Selfreliance\\Adminamazing\\": "src/"}
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --tag=assets --force",
            "php artisan vendor:publish --tag=config --force",
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --tag=assets --force",
            "php artisan vendor:publish --tag=config --force",
        ],
    },
}
